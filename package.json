{
  "name": "ng-webpack-template",
  "version": "18.0.0",
  "description": "A template for angular development using webpack, with some useful batteries included.",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular",
    "hot module reload",
    "ngrx",
    "jest"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "test:debug": "node --inspect --inspect-brk scripts/test.js",
    "test:ci": "node scripts/test.js --no-cache --coverage",
    "build": "node scripts/build.js",
    "build:no-service-worker": "node scripts/build.js --with-service-worker=false",
    "build:no-bo": "node scripts/build.js --use-build-optimizer=false",
    "serve": "node scripts/serve.js",
    "precommit": "lint-staged",
    "prepush": "npm run test:ci",
    "format": "prettier --write \"{src,config,scripts}/**/*.{js,ts}\""
  },
  "lint-staged": {
    "{src,config,scripts}/**/*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.5.13",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/platform-server": "^5.2.10",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@ngrx/store-devtools": "^5.2.0",
    "@ngtools/webpack": "^1.10.2",
    "@types/jest": "^22.2.3",
    "@types/lodash": "4.14.107",
    "@types/lodash-es": "4.17.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "8.4.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "codelyzer": "^4.3.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "detect-port": "^1.2.2",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "filesize": "^3.6.1",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "fs-extra": "^6.0.0",
    "globby": "^8.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.17.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.2",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "jest": "^22.4.3",
    "jest-junit": "^3.7.0",
    "jest-preset-angular": "^5.2.2",
    "js-yaml": "^3.10.0",
    "lint-staged": "^7.0.5",
    "lodash": "^4.17.10",
    "ngrx-store-freeze": "^0.2.2",
    "node-sass": "^4.9.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.12.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "raw-loader": "0.5.1",
    "readline": "^1.3.0",
    "sass-loader": "^7.0.1",
    "script-ext-html-webpack-plugin": "^2.0.0",
    "shelljs": "^0.8.1",
    "source-map-loader": "^0.2.3",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "to-string-loader": "1.1.5",
    "ts-loader": "^3.5.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "uglify-es": "3.3.10",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-chunk-hash": "0.6.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2",
    "workbox-webpack-plugin": "^2.1.3",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngx-translate/core": "^9.1.1",
    "angular-router-loader": "^0.8.5",
    "core-js": "2.5.5",
    "immutable": "3.8.2",
    "lodash-es": "^4.17.10",
    "normalize.css": "^8.0.0",
    "rxjs": "^5.5.10",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">=5.5",
    "yarn": ">=1"
  },
  "jest-junit": {
    "output": "./test-results/junit/junit.xml"
  }
}
