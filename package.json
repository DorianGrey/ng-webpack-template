{
  "name": "ng-webpack-template",
  "version": "19.1.1",
  "description": "A template for angular development using webpack, with some useful batteries included.",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular",
    "hot module reload",
    "ngrx",
    "jest"
  ],
  "main": "index.js",
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.8.1",
    "@angular/compiler-cli": "6.1.7",
    "@angular/platform-server": "6.1.7",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "@ngrx/store-devtools": "^6.1.0",
    "@ngtools/webpack": "6.2.1",
    "@types/jest": "^23.3.1",
    "@types/lodash": "4.14.116",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "9.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "codelyzer": "^4.4.4",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "detect-port": "^1.2.3",
    "file-loader": "^2.0.0",
    "filesize": "^3.6.1",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "fs-extra": "^7.0.0",
    "globby": "^8.0.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy": "^1.17.0",
    "husky": "^1.0.1",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "internal-ip": "^3.0.1",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "jest-preset-angular": "^6.0.0",
    "js-yaml": "^3.12.0",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.2",
    "ngrx-store-freeze": "^0.2.4",
    "node-sass": "^4.9.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "raw-loader": "0.5.1",
    "readline": "^1.3.0",
    "sass-loader": "^7.0.3",
    "script-ext-html-webpack-plugin": "^2.0.0",
    "signale": "^1.2.1",
    "source-map-loader": "^0.2.4",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.0",
    "terser-webpack-plugin": "^1.0.2",
    "to-string-loader": "1.1.5",
    "ts-loader": "5.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "2.9.2",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-chunk-hash": "0.6.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.3",
    "workbox-webpack-plugin": "^3.4.1",
    "yargs": "^12.0.1"
  },
  "dependencies": {
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/forms": "6.1.7",
    "@angular/http": "6.1.7",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@angular/router": "6.1.7",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "angular-router-loader": "^0.8.5",
    "core-js": "2.5.7",
    "immutable": "3.8.2",
    "normalize.css": "^8.0.0",
    "rxjs": "6.3.2",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">=5.5",
    "yarn": ">=1"
  },
  "jest-junit": {
    "output": "./test-results/junit/junit.xml"
  },
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "test:debug": "node --inspect --inspect-brk scripts/test.js",
    "test:ci": "node scripts/test.js --no-cache --coverage",
    "build": "node scripts/build.js",
    "build:no-service-worker": "node scripts/build.js --with-service-worker=false",
    "build:no-bo": "node scripts/build.js --use-build-optimizer=false",
    "serve": "node scripts/serve.js",
    "format": "prettier --write \"{src,config,scripts}/**/*.{js,ts}\"",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix -t codeFrame && stylelint \"src/**/*.scss\" --fix"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -c tslint.json -p tsconfig.json --fix -t codeFrame",
      "prettier --write",
      "git add"
    ],
    "{src,config,scripts}/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  }
}
