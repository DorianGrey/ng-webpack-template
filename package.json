{
  "name": "ng-webpack-template",
  "version": "18.0.0",
  "description": "A template for angular development using webpack, with some useful batteries included.",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular",
    "hot module reload",
    "ngrx",
    "jest"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "test:debug": "node --inspect --inspect-brk scripts/test.js",
    "test:ci": "node scripts/test.js --no-cache --coverage",
    "build": "node scripts/build.js",
    "build:no-service-worker": "node scripts/build.js --with-service-worker=false",
    "build:no-bo": "node scripts/build.js --use-build-optimizer=false",
    "serve": "node scripts/serve.js",
    "precommit": "lint-staged",
    "prepush": "npm run test:ci",
    "format": "prettier --write \"{src,config,scripts}/**/*.{js,ts}\""
  },
  "lint-staged": {
    "{src,config,scripts}/**/*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.6.8",
    "@angular/compiler-cli": "^6.0.6",
    "@angular/platform-server": "^6.0.6",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngtools/webpack": "6.0.8",
    "@types/jest": "^23.1.2",
    "@types/lodash": "4.14.107",
    "@types/lodash-es": "4.17.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "8.6.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "codelyzer": "^4.4.2",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "detect-port": "^1.2.3",
    "file-loader": "^1.1.11",
    "filesize": "^3.6.1",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "fs-extra": "^6.0.1",
    "globby": "^8.0.1",
    "hard-source-webpack-plugin": "^0.9.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy": "^1.17.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.2",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "jest": "^23.2.0",
    "jest-junit": "^5.1.0",
    "jest-preset-angular": "^5.2.3",
    "js-yaml": "^3.12.0",
    "koa": "^2.5.1",
    "koa-compress": "^3.0.0",
    "koa-connect": "^2.0.1",
    "koa-proxies": "^0.7.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.0",
    "ngrx-store-freeze": "^0.2.4",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.6",
    "raw-loader": "0.5.1",
    "readline": "^1.3.0",
    "sass-loader": "^7.0.3",
    "script-ext-html-webpack-plugin": "^2.0.0",
    "signale": "^1.2.1",
    "source-map-loader": "^0.2.3",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "to-string-loader": "1.1.5",
    "ts-loader": "^4.4.1",
    "tslib": "^1.9.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "~2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-chunk-hash": "0.6.0",
    "webpack-merge": "^4.1.3",
    "webpack-serve": "^1.0.4",
    "workbox-webpack-plugin": "^3.3.0",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/forms": "^6.0.6",
    "@angular/http": "^6.0.6",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/router": "^6.0.6",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngx-translate/core": "^10.0.2",
    "angular-router-loader": "^0.8.5",
    "core-js": "2.5.7",
    "immutable": "3.8.2",
    "lodash-es": "^4.17.10",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">=5.5",
    "yarn": ">=1"
  },
  "jest-junit": {
    "output": "./test-results/junit/junit.xml"
  }
}
