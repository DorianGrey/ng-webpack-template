{
  "name": "ng2-webpack-template",
  "version": "0.0.1",
  "description": "Evil brother of ng2-jspm-template",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng2-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular2"
  ],
  "main": "index.js",
  "scripts": {
    "test": "npm run tslint:dist && npm run stylelint:dist && npm run translations:dist && cross-env NODE_ENV=production karma start karma.conf.js",
    "start": "rimraf .tmp && npm run dev-dll && concurrently -r \"npm run dev-qa\" \"npm run translations:dev\" \"wait-for-change src/generated/translations.ts && npm run dev-server\"",
    "dist": "rimraf dist && npm run test && npm run dist-bundle",
    "dist:aot": "rimraf dist-aot && npm run test && npm run dist-bundle:aot",
    "dist-bundle": "cross-env NODE_ENV=production webpack --config config/webpack.dist.config.js",
    "dist-bundle:aot": "npm run dist-bundle -- --env.aot",
    "dist-server": "cross-env NO_ANALYZE_PLUGIN=true npm run dist && node example-dist-server/server.js dist",
    "dist-server:aot": "cross-env NO_ANALYZE_PLUGIN=true npm run dist:aot && node example-dist-server/server.js dist-aot",
    "dev-bundle": "cross-env NODE_ENV=development webpack --config config/webpack.config.js",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.js --inline --hot",
    "dev-qa": "concurrently -r \"npm run translations:dev\" \"npm run stylelint:dev\" \"npm run tslint:dev\" \"karma start karma.conf.js\"",
    "dev-dll": "webpack --config config/webpack.dll.config.js",
    "dev-test": "cross-env NODE_ENV=development karma start karma.conf.js",
    "stylelint:dist": "node bin/lint-styles.js \"src/**/*.scss\"",
    "stylelint:dev": "npm run stylelint:dist -- -w",
    "tslint:dist": "node bin/lint-scripts.js \"src/**/*.ts\" -e \"src/generated/**/*.ts\"",
    "tslint:dev": "npm run tslint:dist -- -w",
    "translations:dist": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "translations:dev": "npm run translations:dist -- -w"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.4",
    "@angular/platform-server": "2.2.4",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@ngtools/webpack": "1.1.6",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.41",
    "@types/node": "6.0.51",
    "angular2-router-loader": "0.3.4",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "commander": "2.9.0",
    "concurrently": "3.1.0",
    "cross-env": "3.1.3",
    "css-loader": "0.26.0",
    "express": "4.14.0",
    "html-webpack-plugin": "2.24.1",
    "http-proxy": "1.15.2",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-reporters": "2.2.0",
    "js-yaml": "3.7.0",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "log4js": "1.0.1",
    "node-sass": "3.13.0",
    "phantomjs-prebuilt": "2.1.13",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "stylelint": "7.6.0",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "tslint": "4.0.2",
    "typescript": "2.0.10",
    "wait-for-change": "1.1.0",
    "webpack": "2.1.0-beta.27",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-server": "2.1.0-beta.11",
    "yargs": "6.4.0"
  },
  "dependencies": {
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/core": "2.2.4",
    "@angular/forms": "2.2.4",
    "@angular/http": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "@angular/router": "3.2.4",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "2.2.1",
    "core-js": "2.4.1",
    "immutable": "3.8.1",
    "lodash": "4.17.2",
    "ng2-translate": "4.0.1",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "0.6.26"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10",
    "yarn": ">=0.17.8"
  }
}
