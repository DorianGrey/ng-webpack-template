{
  "name": "ng-webpack-template",
  "version": "10.0.0",
  "description": "Evil brother of ng2-jspm-template",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular",
    "hot module reload"
  ],
  "main": "index.js",
  "scripts": {
    "clear": "rimraf .awcache .tmp dist dist-aot src/generated",
    "prestart": "rimraf .tmp",
    "start": "npm run dev-dll && concurrently -r \"npm run dev-tasks\" \"wait-for-change src/generated/translations.ts && npm run dev-server\"",
    "pretest": "npm run translations:prod",
    "test": "cross-env NODE_ENV=production karma start karma.conf.js",
    "pretest:dev": "npm run translations:prod",
    "test:dev": "cross-env NODE_ENV=development karma start karma.conf.js",
    "prebuild": "rimraf dist && npm run test",
    "build": "cross-env NODE_ENV=production webpack --config webpack/main.config.js",
    "prebuild:cc": "npm run prebuild",
    "build:cc": "cross-env NODE_ENV=production webpack --config webpack/main.config.js --env.useClosureCompiler",
    "prebuild:aot": "rimraf dist-aot && npm run test",
    "prebuild:aot:bo": "npm run prebuild:aot",
    "build:aot": "cross-env NODE_ENV=production webpack --config webpack/main.config.js --env.useAot",
    "build:aot:bo": "cross-env NODE_ENV=production webpack --config webpack/main.config.js --env.useAot --env.useBo",
    "prebuild:aot:cc": "npm run prebuild:aot",
    "build:aot:cc": "cross-env NODE_ENV=production webpack --config webpack/main.config.js --env.useAot --env.useClosureCompiler",
    "prod-server": "npm run build && node example-prod-server/server.js dist",
    "prod-server:cc": "npm run build:cc && node example-prod-server/server.js dist",
    "prod-server:aot": "npm run build:aot && node example-prod-server/server.js dist-aot",
    "prod-server:aot:bo": "npm run build:aot:bo && node example-prod-server/server.js dist-aot",
    "prod-server:aot:cc": "npm run build:aot:cc && node example-prod-server/server.js dist-aot",
    "dev-bundle": "cross-env NODE_ENV=development webpack --config webpack/main.config.js",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config webpack/main.config.js --hot",
    "dev-tasks": "concurrently -r \"npm run translations:dev\" \"karma start karma.conf.js\"",
    "dev-dll": "webpack --config webpack/dll.config.js",
    "translations:prod": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "translations:dev": "npm run translations:prod -- -w",
    "precommit": "lint-staged",
    "format": "prettier --write \"{src,bin,dev,webpack}/**/*.{js,ts}\""
  },
  "lint-staged": {
    "{src,bin,dev,webpack}/**/*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.4",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/platform-server": "^4.3.1",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@ngrx/store-devtools": "^3.2.4",
    "@ngtools/webpack": "^1.5.1",
    "@types/jasmine": "^2.5.53",
    "@types/lodash-es": "^4.14.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "codelyzer": "^3.1.2",
    "commander": "^2.11.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^0.2.7",
    "html-webpack-plugin": "^2.29.0",
    "http-proxy": "^1.16.2",
    "husky": "^0.14.3",
    "inline-chunk-manifest-html-webpack-plugin": "^1.1.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.4",
    "jasmine-reporters": "^2.2.1",
    "js-yaml": "^3.9.0",
    "karma": "^1.7.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^2.0.4",
    "lint-staged": "^4.0.2",
    "lodash": "^4.17.4",
    "log4js": "^2.2.0",
    "ngrx-store-freeze": "^0.1.9",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^1.5.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "to-string-loader": "1.1.5",
    "ts-loader": "^2.3.1",
    "tslib": "^1.7.1",
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.4.2",
    "wait-for-change": "^1.1.1",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-closure-compiler": "^2.1.5",
    "webpack-dev-server": "^2.5.1",
    "webpack-karma-die-hard": "^1.0.4",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/router-store": "^1.2.6",
    "@ngrx/store": "^2.2.3",
    "@ngx-translate/core": "^7.1.0",
    "@types/lodash": "^4.14.70",
    "angular-router-loader": "^0.6.0",
    "core-js": "2.4.1",
    "immutable": "3.8.1",
