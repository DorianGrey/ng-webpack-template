{
  "name": "ng2-webpack-template",
  "version": "0.0.1",
  "description": "Evil brother of ng2-jspm-template",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng2-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular2"
  ],
  "main": "index.js",
  "scripts": {
    "test": "npm run tslint:dist && npm run stylelint:dist && npm run translations:dist && cross-env NODE_ENV=production karma start karma.conf.js",
    "start": "rimraf .tmp && npm run dev-dll && concurrently -r \"npm run dev-qa\" \"wait-for-change src/generated/translations.ts && npm run dev-server\"",
    "dist": "rimraf dist && npm run test && npm run dist-bundle",
    "dist:aot": "rimraf dist-aot && npm run test && npm run dist-bundle:aot",
    "dist-bundle": "cross-env NODE_ENV=production webpack --config webpack/main.config.js",
    "dist-bundle:aot": "npm run dist-bundle -- --env.aot",
    "dist-server": "cross-env NO_ANALYZE_PLUGIN=true npm run dist && node example-dist-server/server.js dist",
    "dist-server:aot": "cross-env NO_ANALYZE_PLUGIN=true npm run dist:aot && node example-dist-server/server.js dist-aot",
    "dev-bundle": "cross-env NODE_ENV=development webpack --config webpack/main.config.js",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config webpack/main.config.js --inline --hot",
    "dev-qa": "concurrently -r \"npm run translations:dev\" \"npm run stylelint:dev\" \"npm run tslint:dev\" \"karma start karma.conf.js\"",
    "dev-dll": "webpack --config webpack/dll.config.js",
    "dev-test": "cross-env NODE_ENV=development karma start karma.conf.js",
    "stylelint:dist": "node bin/lint-styles.js \"src/**/*.scss\"",
    "stylelint:dev": "npm run stylelint:dist -- -w",
    "tslint:dist": "node bin/lint-scripts.js \"src/**/*.ts\" -e \"src/generated/**/*.ts\"",
    "tslint:dev": "npm run tslint:dist -- -w",
    "translations:dist": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "translations:dev": "npm run translations:dist -- -w"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.1",
    "@angular/platform-server": "^2.4.1",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@ngtools/webpack": "^1.2.1",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.42",
    "@types/node": "^6.0.52",
    "angular2-template-loader": "0.6.0",
    "autoprefixer": "^6.5.4",
    "awesome-typescript-loader": "2.2.4",
    "commander": "2.9.0",
    "concurrently": "3.1.0",
    "cross-env": "3.1.3",
    "css-loader": "^0.26.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "2.24.1",
    "http-proxy": "^1.16.2",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-reporters": "2.2.0",
    "js-yaml": "3.7.0",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-coverage": "^0.1.3",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "log4js": "^1.1.0",
    "node-sass": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.1.1",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "stylelint": "^7.7.0",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "tslint": "^4.1.1",
    "typescript": "2.0.10",
    "wait-for-change": "1.1.0",
    "webpack": "2.1.0-beta.28",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "2.2.1",
    "angular-router-loader": "0.4.0",
    "core-js": "2.4.1",
    "immutable": "3.8.1",
    "lodash": "4.17.2",
    "ng2-translate": "^5.0.0",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.4"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10",
    "yarn": ">=0.17.8"
  }
}
