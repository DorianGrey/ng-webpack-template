{
  "name": "ng-webpack-template",
  "version": "13.0.0",
  "description": "A template for angular development using webpack, with some useful batteries included.",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular",
    "hot module reload",
    "ngrx"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "test:watch": "node scripts/test.js --watch",
    "test:ci": "node scripts/test.js",
    "build": "node scripts/build.js",
    "build:bo": "node scripts/build.js --use-build-optimizer",
    "serve": "node scripts/serve.js",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "format": "prettier --write \"{src,config,scripts}/**/*.{js,ts}\""
  },
  "lint-staged": {
    "{src,config,scripts}/**/*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.16",
