{
  "name": "ng-webpack-template",
  "version": "0.0.1",
  "description": "Evil brother of ng2-jspm-template",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/ng-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "angular2"
  ],
  "main": "index.js",
  "scripts": {
    "test": "npm run tslint:dist && npm run stylelint:dist && npm run translations:dist && cross-env NODE_ENV=production karma start karma.conf.js",
    "start": "rimraf .tmp && npm run dev-dll && concurrently -r \"npm run dev-qa\" \"wait-for-change src/generated/translations.ts && npm run dev-server\"",
    "predist": "rimraf dist && npm run test",
    "dist": "cross-env NODE_ENV=production webpack --config webpack/main.config.js",
    "predist:cc": "rimraf dist && npm run test",
    "dist:cc": "cross-env NODE_ENV=production webpack --config webpack/main.config.js --env.useClosureCompiler",
    "predist:aot": "rimraf dist-aot && npm run test",
    "dist:aot": "cross-env NODE_ENV=production webpack --config webpack/main.config.js --env.useAot",
    "predist:aot:cc": "rimraf dist-aot && npm run test",
    "dist:aot:cc": "cross-env NODE_ENV=production webpack --config webpack/main.config.js --env.useAot --env.useClosureCompiler",
    "dist-server": "npm run dist -- --env.skipAnalyzePlugin && node example-dist-server/server.js dist",
    "dist-server:cc": "npm run dist:cc -- --env.skipAnalyzePlugin && node example-dist-server/server.js dist",
    "dist-server:aot": "npm run dist:aot -- --env.skipAnalyzePlugin && node example-dist-server/server.js dist-aot",
    "dist-server:aot:cc": "npm run dist:aot:cc -- --env.skipAnalyzePlugin && node example-dist-server/server.js dist-aot",
    "dev-bundle": "cross-env NODE_ENV=development webpack --config webpack/main.config.js",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config webpack/main.config.js --hot",
    "dev-qa": "concurrently -r \"npm run translations:dev\" \"npm run stylelint:dev\" \"npm run tslint:dev\" \"karma start karma.conf.js\"",
    "dev-dll": "webpack --config webpack/dll.config.js",
    "dev-test": "cross-env NODE_ENV=development karma start karma.conf.js",
    "stylelint:dist": "node bin/lint-styles.js \"src/**/*.scss\"",
    "stylelint:dev": "npm run stylelint:dist -- -w",
    "tslint:dist": "node bin/lint-scripts.js \"src/**/*.ts\" -e \"src/generated/**/*.ts\"",
    "tslint:dev": "npm run tslint:dist -- -w",
    "translations:dist": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "translations:dev": "npm run translations:dist -- -w"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@ngtools/webpack": "^1.3.0",
    "@types/core-js": "0.6.36",
    "@types/jasmine": "^2.5.46",
    "@types/lodash-es": "4.14.4",
    "@types/node": "7.0.7",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "commander": "2.9.0",
    "concurrently": "^3.4.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-reporters": "^2.2.1",
    "js-yaml": "^3.8.2",
    "karma": "^1.5.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^2.0.3",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "node-sass": "^4.5.1",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.3.3",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.16.0",
    "stylelint": "^7.9.0",
    "to-string-loader": "1.1.5",
    "tslib": "^1.6.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "wait-for-change": "^1.1.1",
    "webpack": "^2.3.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "2.2.1",
    "@ngx-translate/core": "^6.0.1",
    "@types/lodash": "4.14.54",
    "angular-router-loader": "^0.5.0",
    "core-js": "2.4.1",
    "immutable": "3.8.1",
    "lodash-es": "^4.17.4",
    "normalize.css": "^5.0.0",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.5"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10",
    "yarn": ">=0.17.8"
  }
}
